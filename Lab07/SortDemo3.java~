import java.util.*;

public class SortDemo3 {

    public static void main (String [] args) {
        int n = Integer.parseInt(args[0]);
        int [] a = new int[n];

	for (int i = 0; i <n ; i++) {
            a[i] = 0;
	}

        System.out.println(Arrays.toString(a));
        System.out.println("Sort using selection Sort");
        Sorter sorter = new InsertionSort(a);
        sorter.sort();
        System.out.println(Arrays.toString(a));
    }

}